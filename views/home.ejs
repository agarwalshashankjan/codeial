<h1>Welcome to Home</h1>

<section id="feed-posts">
  <h4>Posts</h4>
  <form action="/posts/create" method="post" id="new-post-form">
    <label for="content"> Content: </label>
    <textarea
      name="content"
      id="content"
      placeholder="type here"
      cols="30"
      rows="3"
    ></textarea>
    <input type="submit" value="Post" />
  </form>
</section>
