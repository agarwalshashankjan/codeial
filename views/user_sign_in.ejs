<h1>Sign In</h1>
<form action="users/create_session" method="post">
  <input
    type="email"
    name="email"
    id="email"
    placeholder="Your Email"
    required
  />
  <input
    type="password"
    name="password"
    id="password"
    placeholder="Your Password"
    required
  />
  <input type="submit" value="Sign In" />
</form>
